public class Main {

	public static void main(String[] args) {
		System.out.println("Test 1");
		String ch1 = "bonjour";
		String ch2 = "bonjour";
		if (ch1 == ch2) {
			System.out.println("egales");
		}
		else {
			System.out.println("differentes");
		}
		
		System.out.println("Test 2");
		String c1 = "bonjour";
		String c2 = "bon";
		c2 += "jour";
		if (c1 == c2) {
			System.out.println("egales");
		}
		else {
			System.out.println("differentes");
		}
		
		System.out.println("Test 3");
		String cc1 = "bonjour1", cc2;
		int n = 1;
		cc2 = "bonjour" + n;
		if (cc1 == cc2) {
			System.out.println("egales");
		}
		else {
			System.out.println("differentes");
		}
		
	}
	
}
	
