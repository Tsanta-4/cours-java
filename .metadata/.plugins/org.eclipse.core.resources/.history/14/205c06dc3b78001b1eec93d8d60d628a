public class main {
	public static void main(String[] args) {
		System.out.println("-----------Les Threads---------");
		/// Construction des 3 Threads que nous allons traiter
		Ecrit e1 = new Ecrit ("bonjour", 10);
		Ecrit e2 = new Ecrit ("bonsoir", 12);
		Ecrit e3 = new Ecrit ("\n", 5);
		
		e1.start();
	}
}
